function agregardatos(txt){

	cadena="txt=" + txt;
	$.ajax({
		type:"POST",
		url:"agregarp.php",
		data:cadena,
		success:function(r){
			if(r==1){
				$('#tabla').load('publicaciones.php');
				alertify.success("agregado con exito :)");
			}else{
				alertify.error("Fallo el servidor :(");
			}
		}
	});

}